  "author": "Gustavo M T Borges",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nogueira-nobre/nogueira-nobre-admin-api/issues"
  },
  "homepage": "https://github.com/nogueira-nobre/nogueira-nobre-admin-api#readme",
  "dependencies": {
    "bcrypt": "1.0.2",
    "body-parser": "^1.16.1",
    "express": "^4.14.1",
    "jwt-simple": "^0.5.1",
    "mongoose": "^4.8.3",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-preset-node6": "^11.0.0",
    "chai": "^3.5.0",
