    "test:i": "mocha --opts test/integration/mocha.opts test/integration/**/*_spec.js",
    "test:c": "mocha --opts test/contract/mocha.opts test/contract/**/*_spec.js",
    "lint": "eslint '**/*.js'",
    "lint:fix": "eslint '**/*.js' --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nogueira-nobre/nogueira-nobre-admin-api.git"
  },
  "keywords": [
    "nogueira",
    "nobre",
    "admin",
    "rest",
    "licitacao"
